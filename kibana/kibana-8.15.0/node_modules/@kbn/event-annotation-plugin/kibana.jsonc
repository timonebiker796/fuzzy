{
  "type": "plugin",
  "id": "@kbn/event-annotation-plugin",
  "owner": [
    "@elastic/kibana-visualizations"
  ],
  "description": "The Event Annotation service contains expressions for event annotations",
  "plugin": {
    "id": "eventAnnotation",
    "browser": true,
    "server": true,
    "requiredPlugins": [
      "expressions",
      "data",
      "presentationUtil",
      "visualizations",
      "dataViews",
      "unifiedSearch",
      "kibanaUtils",
      "contentManagement"
    ],
    "optionalPlugins": [
      "savedObjectsTagging"
    ],
    "requiredBundles": [
      "data",
      "savedObjectsFinder",
      "dataViews"
    ],
    "extraPublicDirs": [
      "common"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}