{
  "type": "plugin",
  "id": "@kbn/vis-default-editor-plugin",
  "owner": [
    "@elastic/kibana-visualizations"
  ],
  "description": "The default editor used in most aggregation-based visualizations.",
  "plugin": {
    "id": "visDefaultEditor",
    "browser": true,
    "server": false,
    "requiredPlugins": [
      "dataViews",
      "unifiedSearch"
    ],
    "optionalPlugins": [
      "visualizations"
    ],
    "requiredBundles": [
      "kibanaUtils",
      "kibanaReact",
      "data",
      "fieldFormats",
      "esUiShared",
      "visualizations",
      "savedSearch"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}