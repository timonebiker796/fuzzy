{
  "type": "plugin",
  "id": "@kbn/unified-doc-viewer-plugin",
  "owner": [
    "@elastic/kibana-data-discovery"
  ],
  "description": "This plugin contains services reliant on the plugin lifecycle for the unified doc viewer component (see @kbn/unified-doc-viewer).",
  "plugin": {
    "id": "unifiedDocViewer",
    "browser": true,
    "server": false,
    "requiredPlugins": [
      "data",
      "discoverShared",
      "fieldFormats"
    ],
    "optionalPlugins": [
      "fieldsMetadata"
    ],
    "requiredBundles": [
      "kibanaUtils"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}