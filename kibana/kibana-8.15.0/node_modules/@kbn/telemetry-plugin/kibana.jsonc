{
  "type": "plugin",
  "id": "@kbn/telemetry-plugin",
  "owner": [
    "@elastic/kibana-core"
  ],
  "plugin": {
    "id": "telemetry",
    "browser": true,
    "server": true,
    "requiredPlugins": [
      "telemetryCollectionManager",
      "usageCollection",
      "screenshotMode"
    ],
    "optionalPlugins": [
      "home",
      "security"
    ],
    "requiredBundles": [
      "kibanaUtils"
    ],
    "enabledOnAnonymousPages": true,
    "extraPublicDirs": [
      "common/constants"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}