{
  "type": "plugin",
  "id": "@kbn/controls-plugin",
  "owner": [
    "@elastic/kibana-presentation"
  ],
  "description": "The Controls Plugin contains embeddable components intended to create a simple query interface for end users, and a powerful editing suite that allows dashboard authors to build controls",
  "plugin": {
    "id": "controls",
    "browser": true,
    "server": true,
    "requiredPlugins": [
      "presentationUtil",
      "kibanaReact",
      "expressions",
      "embeddable",
      "dataViews",
      "data",
      "unifiedSearch",
      "uiActions"
    ],
    "requiredBundles": [
      "kibanaUtils"
    ],
    "extraPublicDirs": [
      "common"
    ],
    "__category__": {
      "oss": true,
      "example": false,
      "testPlugin": false
    }
  }
}