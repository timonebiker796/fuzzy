{
  "type": "plugin",
  "id": "@kbn/logs-shared-plugin",
  "owner": [
    "@elastic/obs-ux-logs-team"
  ],
  "description": "Exposes the shared components and APIs to access and visualize logs.",
  "plugin": {
    "id": "logsShared",
    "browser": true,
    "server": true,
    "configPath": [
      "xpack",
      "logs_shared"
    ],
    "requiredPlugins": [
      "data",
      "dataViews",
      "discoverShared",
      "usageCollection",
      "observabilityShared",
      "share"
    ],
    "optionalPlugins": [
      "observabilityAIAssistant"
    ],
    "requiredBundles": [
      "kibanaUtils",
      "kibanaReact"
    ],
    "extraPublicDirs": [
      "common"
    ],
    "__category__": {
      "oss": false,
      "example": false,
      "testPlugin": false
    }
  }
}